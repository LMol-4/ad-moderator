{
  "name": "@gabriel/ad-moderator",
  "version": "1.0.1",
  "description": "A TypeScript library for detecting images not eligible for public ad spaces with pluggable analysis functions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist",
    "publish:lib": "npm run build && npm publish --registry https://registry.npmjs.org/"
  },
  "keywords": [
    "ad-moderation",
    "content-moderation",
    "image-classification",
    "machine-learning",
    "typescript",
    "npm-library",
    "pluggable",
    "image-analysis",
    "moderation",
    "claude",
    "ai"
  ],
  "author": "Gabriel <gabriel@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ad-moderator.git"
  },
  "homepage": "https://github.com/yourusername/ad-moderator#readme",
  "bugs": {
    "url": "https://github.com/yourusername/ad-moderator/issues"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.1",
    "axios": "^1.6.0",
    "canvas": "^2.11.2",
    "form-data": "^4.0.0",
    "sharp": "^0.33.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
